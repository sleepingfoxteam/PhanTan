<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>TUIBANK</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/master.css")
</head>
<body>
    <div class="my-nav">
        <div class="div-logo">
            @Html.ActionLink("TUIBANK", "Index", "Home", new { Class = "logo" })
        </div>
        <ul class="nav-ul">
            <li><a>Khach Hang</a></li>
            <li><a>Tai Khoan</a></li>
            <li><a class="text-red">Dang Xuat</a></li>
        </ul>
    </div>
    <div class="my-body" style="background:magenta;">
        @RenderBody()
        body
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
